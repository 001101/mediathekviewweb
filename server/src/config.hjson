{
  dataDirectoriy: "./data"

  redis: {
    host: localhost
    port: 6379
    db: 0
  }

  importer: {
    #interval in seconds to check for an updated "latest filmlist" (Filmliste-akt - for people who know that)
    latestCheckInterval: 120

    #timeout (reset after every latest check) after which to do a full check from filmlist archive
    fullCheckTimeout: 60 * 45

    cache: true
  }
}
