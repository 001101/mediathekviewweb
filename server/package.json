{
  "name": "mediathekviewweb-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node ./dist/app.js",
    "inspect": "node --inspect ./dist/app.js",
    "inspect-min-memory": "node --inspect --optimize-for-size --memory-reducer --clear-free-memory --use-idle-notification ./dist/app.js",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "build-schema": "typescript-json-schema --include src/common/search-engine/query/definition.ts --out src/schemas/search-query.json --aliasRefs --titles --noExtraProps --required tsconfig.json SearchQuery",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "author": "Patrick Hein",
  "license": "GPLv3",
  "dependencies": {
    "ajv": "^6.9.1",
    "elasticsearch": "^15.4.1",
    "ioredis": "^4.6.2",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "lzma-native": "^4.0.3",
    "mongodb": "^3.1.13",
    "needle": "^2.2.4",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@types/elasticsearch": "^5.0.30",
    "@types/ioredis": "^4.0.6",
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.39",
    "@types/lzma-native": "^4.0.0",
    "@types/mongodb": "^3.1.19",
    "@types/needle": "^2.0.4",
    "@types/node": "^10.12.24",
    "typescript": "^3.3.3",
    "typescript-json-schema": "^0.35.0"
  }
}
