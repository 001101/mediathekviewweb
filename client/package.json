{
  "name": "mediathekviewweb",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --preserve-symlinks --extract-css false --proxy-config proxy.conf.json --poll 1000 --host 0.0.0.0",
    "build": "ng build --prod --preserve-symlinks --extract-css false",
    "lint": "ng lint",
    "analyze": "ng build --prod --preserve-symlinks --extract-css false --sourcemaps true --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.7",
    "@angular/cdk": "^6.3.2",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/material": "^6.3.2",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "6.0.7",
    "@angular/service-worker": "6.0.7",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "rxjs": "^6.2.1",
    "tslint": "^5.10.0",
    "video.js": "^6.10.3",
    "videojs-contrib-hls": "^5.14.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@types/video.js": "^6.2.7",
    "typescript": "2.7.2",
